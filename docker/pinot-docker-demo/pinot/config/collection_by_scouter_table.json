{
    "tableName": "collection_by_scouter",
    "tableType": "REALTIME",
    "segmentsConfig": {
        "timeColumnName": "updated_at",
        "timeType": "DAYS",
        "retentionTimeUnit": "DAYS",
        "retentionTimeValue": "60",
        "segmentPushType": "APPEND",
        "schemaName": "collection_by_scouter",
        "allowNullTimeValue": false,
        "replication": "1",
        "replicasPerPartition": "1"
    },
    "tenants": {},
    "tableIndexConfig": {
        "loadMode": "MMAP",
        "streamConfigs": {
            "streamType": "kafka",
            "stream.kafka.consumer.type": "lowlevel",
            "stream.kafka.topic.name": "collection_by_scouter",
            "stream.kafka.decoder.class.name": "org.apache.pinot.plugin.stream.kafka.KafkaJSONMessageDecoder",
            "stream.kafka.consumer.factory.class.name": "org.apache.pinot.plugin.stream.kafka20.KafkaConsumerFactory",
            "stream.kafka.hlc.zk.connect.string": "kafka-zookeeper-headless.pinot.svc.cluster.local:2181",
            "stream.kafka.zk.broker.url": "kafka-zookeeper-headless.pinot.svc.cluster.local:2181",
            "stream.kafka.broker.list": "kafka-headless.pinot.svc.cluster.local:9092",
            "realtime.segment.flush.threshold.time": "3600000",
            "realtime.segment.flush.threshold.size": "50000",
            "stream.kafka.consumer.prop.auto.offset.reset": "smallest"
        }
    },
    "metadata": {
        "customConfigs": {}
    },
    "routing": {
        "instanceSelectorType": "strictReplicaGroup"
    },
    "upsertConfig": {
        "mode": "FULL"
    }
}